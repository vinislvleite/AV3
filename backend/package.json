{
  "name": "av3-backend",
  "version": "1.0.0",
  "main": "server.ts",
  "scripts": {
    "db:push": "npx prisma db push --force-reset",
    "db:generate": "npx prisma generate",
    "db:seed": "tsx prisma/seed.ts",
    "db:prep": "npm run db:push && npm run db:generate && npm run db:seed",
    "dev": "npm run db:prep && tsx watch server.ts",
    "start": "tsx server.ts"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "bcryptjs": "^3.0.3",
    "cors": "^2.8.5",
    "express": "^5.1.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/node": "^22.19.1",
    "prisma": "^5.22.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  }
}